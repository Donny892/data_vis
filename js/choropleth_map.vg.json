{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Population Density of Australian Statistical Areas Level",
  "width": 800,
  "height": 400,
  "projection": {"type": "equalEarth"},
  "data": {
    "url": "SA3_2016_AUST.topojson",
    "format": {"type": "topojson", "feature": "SA3_2016_AUST"}
  },
  "transform": [
    {
      "lookup": "properties.SA3_NAME16",
      "from": {
        "data": {
          "url": "SA3_data.csv"
        },
        "key": "SA3_NAME16",
        "fields": ["Population"]
      }
    },
    {"calculate": "datum.Population", "as": "Active Cases"}
  ],
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "Active Cases",
      "type": "quantitative",
      "scale": {"type": "log"}
    },
    "tooltip": [
      {"field": "properties.SA3_NAME16", "type": "nominal", "title": "Country"},
      {"field": "Population", "type": "quantitative"}
    ]
  }
}